{
    "pages": [
        {
            "path": "pages/home"
        },
        {
            "path": "pages/$module_name_mobile_short$/page1"
        },
        {
            "path": "pages/$module_name_mobile_short$/page2"
        }
    ],
    "easycom": {
        "autoscan": true,
        "custom": {
            "^c-(.*)": "@/brick/components/c-$1.vue",
            "^tui-(.*)": "@/brick/tui-components/tui-$1/tui-$1.vue",
            "^bui-(.*)": "@/brick/bui/bui-$1/bui-$1.vue",
            "^uni-(.*)": "@/brick/uni/uni-$1/uni-$1.vue",
            "^app-(.*)": "@/components/app-$1.vue"
        }
    },
    "globalStyle": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "black",
        "navigationBarBackgroundColor": "#FFFFFF",
        "pullToRefresh": {
            "color": "#579288",
            "offset": 50
        }
    }
}
